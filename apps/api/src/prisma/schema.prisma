generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Generation {
  id        String   @id @default(uuid())
  figmaUrl String
  framework String
  code      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

